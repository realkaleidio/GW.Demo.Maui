<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="GW.Demo.Maui.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:GW.Demo.Maui"
    xmlns:views="clr-namespace:GW.Demo.Maui.Views"
    Title="GW.Demo.Maui">

    <ShellContent Title=".NET MAUI Demo" Route="swipe" ContentTemplate="{DataTemplate views:SwipeHostPage}" />

    <MenuItem Text="Position" Command="{Binding SelectCommand}" CommandParameter="0" />
    <MenuItem Text="Posts" Command="{Binding SelectCommand}" CommandParameter="1" />
    <MenuItem Text="Video" Command="{Binding SelectCommand}" CommandParameter="2" />
    <MenuItem Text="Foto" Command="{Binding SelectCommand}" CommandParameter="3" />
    <MenuItem Text="Kontakte" Command="{Binding SelectCommand}" CommandParameter="4" />
</Shell>
